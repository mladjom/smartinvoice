{
    "name": "mladjom/smartinvoice",
    "description": "Invoicing application built with Laravel 4.2.6 and Bootstrap 3.2.0.",
    "keywords": ["invoice", "laravel", "bootstrap", "invoicing"],
    "license": "MIT",
    "require": {
        "laravel/framework": "4.2.*",
        "zizaco/confide": "~4.0@dev",
        "zizaco/entrust": "1.2.*@dev",
        "chumper/datatable": "2.x",
        "omnipay/omnipay": "2.x",
        "webpatser/laravel-countries": "dev-master",
        "intervention/image": "2.x",
        "h4cc/wkhtmltopdf-amd64": "0.12.0",
        "h4cc/wkhtmltoimage-amd64": "0.12.0",
        "barryvdh/laravel-snappy": "0.1.x"
    },
    "require-dev": {
        "phpunit/phpunit": "4.0.*",
        "way/generators": "~2.0",
        "orangehill/iseed": "dev-master",
        "barryvdh/laravel-debugbar": "dev-master",
        "fzaninotto/faker": "1.4.0"
    },
    "autoload": {
        "classmap": [
            "app/commands",
            "app/controllers",
            "app/models",
            "app/database/migrations",
            "app/database/seeds",
            "app/tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "stable"
}
